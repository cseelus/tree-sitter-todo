{
  "name": "todo",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "project"
          },
          {
            "type": "SYMBOL",
            "name": "todo_done"
          },
          {
            "type": "SYMBOL",
            "name": "todo_cancelled"
          },
          {
            "type": "SYMBOL",
            "name": "todo_pending"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "empty_line"
          }
        ]
      }
    },
    "project": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "project_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "SYMBOL",
                  "name": "tags"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "project_name": {
      "type": "PATTERN",
      "value": "[^\\n\\r:]+"
    },
    "todo_pending": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "pending_symbol"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "task_content"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "todo_done": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "done_symbol"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "task_content"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "todo_cancelled": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "cancelled_symbol"
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "SYMBOL",
          "name": "task_content"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "pending_symbol": {
      "type": "STRING",
      "value": "☐"
    },
    "done_symbol": {
      "type": "STRING",
      "value": "✔"
    },
    "cancelled_symbol": {
      "type": "STRING",
      "value": "✘"
    },
    "task_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "tag"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "tag_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tag_value_group"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_-]*"
    },
    "tag_value_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "tag_value"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "tag_value": {
      "type": "PATTERN",
      "value": "[^)\\n\\r]+"
    },
    "tags": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "tag"
      }
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n\\r@]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_indent"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "comment_text"
        },
        {
          "type": "SYMBOL",
          "name": "_eol"
        }
      ]
    },
    "comment_text": {
      "type": "PATTERN",
      "value": "[^☐✔✘\\n\\r][^\\n\\r]*"
    },
    "_indent": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_eol": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "empty_line": {
      "type": "PATTERN",
      "value": "\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\r"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
